<xs:schema 	xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns="https://awebergmbh.ch/XMLSchema"
			targetNamespace="https://awebergmbh.ch/XMLSchema"
			elementFormDefault="qualified">
	<xs:element name="Surveydata">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Versiondata">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="TypeOfRecording" type="xs:string"/> <!-- Stichprobe Betriebsführung (Bezeichung der Stichprobe) -->
							<xs:element name="Creationdate" type="xs:string"/>
							<xs:element name="WrittenBySoftware" type="xs:string"/> <!-- Name der Sofware, welches das lieferende XML-File erstellt -->
							<xs:element name="SoftwareversionUsed" type="xs:string"/>
							<xs:element name="XMLFormatVersion" type="xs:string"/> <!-- Version der XML-Definition der Zentralen Auswertung-->
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Farmdata">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="TrustofficeId" type="xs:integer" minOccurs="0"/> <!-- Optionales Merkmal | Diese ZA-Treuhandstellenindentifiktionsnummer wird durch die ZA einmalig vergeben-->
							<xs:element name="Trustofficename" type="xs:string" minOccurs="0"/> <!-- Optinales Merkmal-->
							<xs:element name="FarmId" type="xs:integer"/> <!-- Identifikation des Betriebes, soll im laufe der Jahre nicht verändert werden -->
							<xs:element name="Period" type="PeriodRestriction"/> <!-- Buchhaltungsjahr z.B. 2014 | Grundsätzlich wird eine Periode vom 1.1 bis 31.12 seitens ZA gewünscht --> 							
							<xs:element name="Farmname" type="xs:string" minOccurs="0" /> <!--  Optionales Merkal | z.B. Name und Vorname des Betriebsleiters zur Ereknnung des Betriebes im ZA-Erhebungstool. Dies kann man aber auch über die FarmId machen -->
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="FinancialAccounting">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AccountStructure">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Account" maxOccurs="unbounded" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Accountnumber" type="xs:string"/> <!-- Betriebsspezifische FiBu-Nummerierung | allfällige zusätzliche Buchungen im Erhebungstool sind mit der eigenen Nummerierung zu machen-->
													<xs:element name="Accountname" type="xs:string"/> <!-- Betriebssepzifischer Name des FiBu-Kontos -->
													<xs:element name="IsAccount" type="xs:boolean"/> <!-- Konto oder Gliederungsposition möglich-->
													<xs:element name="AccountnumberZA" type="AccountnumberZARestriction" minOccurs="0"/> <!-- optionales Merkmal | Liefert das Mapping zu der ZA-Kontennummer | Kann im ZA-Erhebungstool auch mittels Vorlagen zugeteilt und betriebsspezifisch angepasst werden-->
													<xs:element name="AccountnameZA" type="xs:string" minOccurs="0"/> <!-- optionales Merkmal | Wird nicht direkt verwendet -->
													<xs:element name="Level" type="xs:integer" minOccurs="0"/> <!-- optionales Merkmal | Abbildung Hierarchiestufe - oberstes Element hat den Wert 0 -->
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="CostunitStructure">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Costunit" maxOccurs="unbounded" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Costunitnumber" type="xs:string"/> <!-- Betriebsspezifische Kostenträgernummer | allfällige zusätzliche Buchungen im Erhebungstool sind mit der eigenen Nummerierung zu machen -->
													<xs:element name="Costunitname" type="xs:string"/> <!-- Betriebsspezifische Kostenträger-Bezeichung -->
													<xs:element name="IsCostunit" type="xs:boolean"/> <!-- Kostenträger oder Gliederung möglich-->
													<xs:element name="CostunitnumberZA" type="CostunitnumberZARestriction" minOccurs="0"/> <!-- Optionales Merkmal | Liefert das Mapping zum ZA-Kostenträger | Kann im ZA-Erheungstool auch mittels Vorlage zugeteilt und betriebsspezifisch angepasst werden -->
													<xs:element name="CostunitnameZA" type="xs:string" minOccurs="0"/> <!-- Optionales Merkmal | Wird nicht direkt verwendet -->
													<xs:element name="Level" type="xs:integer" minOccurs="0"/> <!-- optionales Merkmal | Abbildung Hierarchiestufe - oberstes Element hat den Wert 0 -->
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Journalentries"> <!-- Buchungsjournal des Betriebes | Inklusive Einbilanzierungsbuchungen sowie internen Lieferungen (z.B. Gerste an Schweine) | Die Kostenträgerzuteilung erfolgt unter "JournalentriesCostunit"-->
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Journalitem" maxOccurs="unbounded" minOccurs="1">
											<xs:complexType>
												<xs:sequence>
													<!-- <xs:element name="Sector" type="SectorRestriction"/> -->
													<!--  Buchungssektor sind unter Restriction definiert | 
													Unterteilt wird in FiBu Buchungen und Buchungen ausserhalb der FiBu |
													Interne Lieferungen sind z.B. keine FiBu Buchungen -->
													<xs:element name="EntryId" type="xs:integer"/> <!-- Dient zur Identifikation des einzelen Journaleintrages | Nummer darf nicht dopplet vorkommen | Basis für Verknüpfung mit der Kostenträgerzuteilung "JournalentriesCostUnit"-->
													<xs:element name="Receiptnumber" type="xs:string" minOccurs="0"/> <!-- Optionales Merkmal | Kann zur Erkennung von Buchungen dienen, da die Buchungstexte nicht mitgeliefert werden -->
													<xs:element name="Entrydate" type="xs:string"/> 
													<xs:element name="Amount" type="xs:float"/>
													<xs:element name="AccountnumberDebit" type="xs:string"/> <!-- Soll-Kontonummer -->
													<xs:element name="AccountnumberCredit" type="xs:string"/> <!-- Haben-Kontonummer -->
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="JournalentriesCostunit"> <!-- Dient als Schlüssel zwischen Buchung und Kostenträgerzuteilung | Nicht jede Buchung muss eine Kostentträgerzuteilung haben | Ein Buchung darf mehrere Kostenträgerzuteilungen haben -->
								<xs:complexType>
									<xs:sequence>
										<xs:element name="JournalitemCostunit" maxOccurs="unbounded" minOccurs="1">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="EntryCostunitItemId" type="xs:integer" minOccurs="0"/> <!-- Optionales Merkmal | Kann anlog Belegnummer zur Wiedererkennung eines einzelenen Eintrages dienen -->
													<xs:element name="EntryNr" type="xs:integer"/> <!-- EntryId des Journalitem's, welches in Verbindung mit diesem Eintrag steht -->
													<xs:element name="Amount" type="xs:float"/> <!-- Betrag oder Teilbetrag der diesem Kostenträger zugeteilt wird -->
													<xs:element name="CostunitnumberDebit" type="xs:string" minOccurs="0"/> <!-- mindestens CostunitnumberDebit oder CostunitnumberCredit notwendig-->
													<xs:element name="CostunitnumberCredit" type="xs:string" minOccurs="0"/> <!-- mindestens CostunitnumberDebit oder CostunitnumberCredit notwendig-->
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="NonFinancialAccounting"><!-- Non Financial Accounting data  -->
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AttributeList" maxOccurs="unbounded" minOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Attribute" maxOccurs="unbounded" minOccurs="1">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="UID" type="xs:string" minOccurs="1"/><!-- Dient zur Identifikation des Merkmals  --> 
													<xs:element name="Index" type="xs:integer" minOccurs="0"/><!-- Optional z.B. für Arbeitskräfte  --> 
													<xs:element name="Datatype" type="xs:integer" minOccurs="0"/><!-- Optional --> 
													<xs:element name="Value" type="xs:string" minOccurs="1"/><!-- Haltet des Wert des Merkmals - die Datentypen wurden zu Text konvertiert --> 
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>


<!-- ZA - Restrictions  -->
	<!-- Period-->
	<xs:simpleType name="PeriodRestriction">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="2014"/>
			<xs:maxInclusive value="2099"/>
		</xs:restriction>
	</xs:simpleType>
		
	<!-- ZA Sectors -->
	<xs:simpleType name="SectorRestriction">
		<xs:restriction base="xs:integer">
			<xs:enumeration value="1"/> <!-- FiBu-Buchung -->
			<xs:enumeration value="2"/> <!-- keine FiBu-Buchung | z.B. interne Lieferungen -->
		</xs:restriction>
	</xs:simpleType>

